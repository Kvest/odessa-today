<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:paddingRight="9dip">
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid
                android:color="#f5f5f5" />
            <size
                android:height="16dip"
                android:width="16dip" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="ring"
            android:innerRadius="7dip"
            android:thickness="1dip"
            android:useLevel="false">
            <solid
                android:color="#c3c3c3" />
        </shape>
    </item>
    <item
        android:gravity="center">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid
                android:color="#24282b" />
            <size
                android:height="6dip"
                android:width="6dip" />
        </shape>
    </item>
</layer-list>